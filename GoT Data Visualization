a = battlesGoTstats
library(ggplot2)
library(vcd)
install.packages('vcd')
library(vcd)
library(alluvial)
install.packages('RColorBrewer')
library(RColorBrewer)
install.packages('seriation')
library(seriation)
install.packages('GGally')
library(GGally)
tit <- as.data.frame(a)
tit2d <- aggregate(Degree~Id+modularity_class,data = tit,sum)
alluvial(tit2d[,1:2],freq = tit2d$Degree,col = ifelse(tit2d$modularity_class=='0','blue','grey'))
myplot <- ggplot(data=battlesGoTstats, aes(x=Degree,y=pageranks))
myplot <- myplot + facet_wrap(~Id)+geom_point(aes(colour=betweenesscentrality,size=modularity_class),alpha = 0.6)
myplot
ggplot(data=a)+geom_bar(aes(x=modularity_class,y=..count..,fill = Id),alpha = 0.7)
ggplot(data = a,aes(x=modularity_class,fill=Id))+theme_bw()+geom_density(alpha=0.7)+xlab("Modularity Class")+ylab("Density")
myplot1<-ggplot(data=a)
myplot1+geom_point(aes(x=modularity_class,y=Degree,colour=Id,size=outdegree))
myplot2<-ggplot(data=a)
myplot2+geom_histogram(aes(x=clustering, fill = Id),alpha = 0.7)
myplot3 <-ggplot(data=a)
myplot3+geom_point(aes(x=betweenesscentrality,y=modularity_class),alpha=0.3)
